<%= form_for dr do |f| %>
  <div class="form-group">
    <%= f.label "What do you need?"%>
    <%= f.text_field :items, class: "form-control", placeholder: "i.e. grapes, eggs, etc." %>
  </div>
  <div class="form-group">
    <%= f.label "When would you like this delivered?" %>
    <%= f.text_field :requested_date, class: "form-control", placeholder: "Please enter in MM/DD/YYYY format" %>
  </div>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
